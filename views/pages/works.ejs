<%- include('../template/header') -%>
<!-- Wrapper -->
<div class="wrapper">	

    <!-- Section Started -->
    <div class="section started section-title" id="section-started">

        <!-- background -->
        <div class="video-bg">
            <div class="video-bg-mask"></div>
            <div class="video-bg-texture" id="grained_container"></div>
        </div>

        <!-- started content -->
        <div class="centrize full-width">
            <div class="vertical-center">
                <div class="started-content">
                    <h1 class="h-title"><%- header.nav.works %></h1>
                    <div class="h-subtitles">
                        <div class="h-subtitle typing-bread">
                            <p class="breadcrumbs">
                                <a href="/"><%- header.nav.home %></a> / <%- header.nav.works %>
                            </p>
                        </div>
                        <span class="typed-bread"></span>
                    </div>
                </div>
            </div>
        </div>

        <!-- mosue button -->
        <a href="#" class="mouse_btn" style="display: none;"><span class="icon fas fa-chevron-down"></span></a>

    </div>

    <!-- Works -->
    <div class="section works" id="section-portfolio">
        <div class="content">
        
            <!-- portfolio filter -->
            <div class="filter-menu content-box">
                <div class="filters">
                    <div class="btn-group">
                        <label data-text="All" class="glitch-effect"><input type="radio" name="fl_radio" value=".box-item">All</label>
                    </div>
                    <div class="btn-group">
                        <label data-text="Web Development"><input type="radio" name="fl_radio" value=".f-web">Web Development</label>
                    </div>
                    <div class="btn-group">
                        <label data-text="Spigot Plugins"><input type="radio" name="fl_radio" value=".f-spigot">Spigot Plugins</label>
                    </div>
                </div>
            </div>
            
            <!-- portfolio items -->
            <div class="box-items portfolio-items">

                <div class="box-item f-spigot">
                    <div class="image">
                        <a href="#backpacks" class="has-popup-media hover-animated">
                            <img src="images\backpack.jpg" class="wp-post-image" alt="">
                            <span class="info circle">
                                <span class="centrize full-width">
                                    <span class="vertical-center">
                                        <span class="icon fas fa-plus"></span>
                                        <span class="desc">
                                            <span class="category">Spigot Plugin</span>
                                            <span class="name">Backpacks</span>
                                        </span>
                                    </span>
                                </span>
                            </span>
                        </a>
                    </div>
                    <div id="backpacks" class="popup-box mfp-fade mfp-hide">
                        <div class="content">
                            <div class="image" style="background-image: url(images/backpack.jpg);"></div>
                            <div class="desc single-post-text">
                                <div class="category">Spigot Plugin</div>
                                <h4>Backpacks</h4>
                                <a href="https://www.spigotmc.org/resources/backpacks.80583/" class="btn hover-animated">
                                    <span class="circle"></span>
                                    <span class="lnk">View Project</span>
                                </a>
                            </div>
