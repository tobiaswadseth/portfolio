<!doctype html>
<html lang="en-US">
<head>

	<!-- Meta -->
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
	<meta name="description" content="Tobias Wadseth - Java developer">
	<meta name="keywords" content="tobias, wadseth, tobias wadseth, portfolio, development, web, ui, ux, ui/ux, design, software, programming">
	<meta name="author" content="Tobias Wadseth">

	<!-- Title -->
	<title>Tobias Wadseth</title>

	<!-- Fonts -->
	<link href="https://fonts.googleapis.com/css?family=Roboto:100,100i,300,300i,400,400i,500,500i,700,700i,900,900i&display=swap&subset=cyrillic" rel="stylesheet">

	<link rel="stylesheet" href="../css/styles.min.css">
	<link rel="stylesheet" href="../css/all.css">

</head>

<body>

	<!-- Preloader -->
	<div class="preloader">
		<div class="centrize full-width">
			<div class="vertical-center">
				<div class="spinner">
					<div class="double-bounce1"></div>
					<div class="double-bounce2"></div>
				</div>
			</div>
		</div>
	</div>
	
	<!-- Container -->
	<div class="container">

		<!-- Cursor -->
		<div class="cursor-follower"></div>
	
		<!-- Header -->
		<header class="header">
			<div class="head-top">

				<!-- menu button -->
				<a href="#" class="menu-btn"><span></span></a>

				<!-- logo -->
				<div class="logo hover-masks-logo">
					<a href="./files/cv">
						<span class="mask-lnk">Tobias <strong>Wadseth</strong></span>
						<span class="mask-lnk mask-lnk-hover"><%- header.download %> <strong>CV</strong></span>
					</a>
				</div>
				
				<!-- top menu -->						
				<div class="top-menu hover-masks">
					<div class="top-menu-nav">	
						<div class="menu-topmenu-container">
							<ul class="menu">
								<li class="menu-item <%= page == 'home' ? 'current-menu-item' : ''; %> ">
									<a href="/"><%- header.nav.home %></a>
								</li>
								<li class="menu-item <%= page == 'resume' ? 'current-menu-item' : ''; %>">
									<a href="/resume"><%- header.nav.resume %></a>
								</li>
								<li class="menu-item <%= page == 'works' ? 'current-menu-item' : '' %>">
									<a href="/works"><%- header.nav.works %></a>
								</li>
								<li class="menu-item <%= page == 'contact' ? 'current-menu-item' : ''; %>">
									<a href="/contact"><%- header.nav.contact %></a>
								</li>
							</ul>
						</div>
					</div>
				</div>

			</div>
		</header>